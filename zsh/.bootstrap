#
# Bootstrap
# --------------------------------------------------------------------

for file in $DOTFILES_ZSH/.{exports,aliases,functions}; do
  [ -r "$file" ] && source "$file"
done
unset file

[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm

if which rbenv > /dev/null; then eval "$(rbenv init -)"; fi
if which fuck > /dev/null; then eval $(thefuck --alias); fi
