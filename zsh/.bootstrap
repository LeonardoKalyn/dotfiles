#
# Bootstrap
# --------------------------------------------------------------------

for file in $DOTFILES_ZSH/.{exports,aliases,functions}; do
  [ -r "$file" ] && source "$file"
done
unset file

[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm

eval "$(rbenv init -)"
eval $(thefuck --alias)
